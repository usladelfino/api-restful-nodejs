{
  "source": {
    "include": ["./src"],
    "exclude": ["./node_modules", "./__tests__", "./vscode"]
  },
  "sourceType": "module",
  "plugins": [],
  "recurseDepth": 10,
  "opts": {
    "recurse": true,
    "destination": "./docs",
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
